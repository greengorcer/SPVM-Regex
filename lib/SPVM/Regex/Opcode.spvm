class Regex::Opcode {
  allow Regex;
  
  enum {
    RE_START,
    RE_END,
    PATTERN,
    CAPTURE_START,
    CAPTURE_END,
  }
  has code_point_ranges : int[];
  has negate : byte;
  has min_repeat : int;
  has max_repeat : int;
  has capture_begin : byte;
  has capture_end : byte;
  has string_or_line_begin : byte;
  has string_or_line_end : byte;
  has next_opcodes : Regex::Opcode[];
}
